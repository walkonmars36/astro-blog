---
// library imports
import {Icon} from "astro-icon";

// data imports
import navData from "../data/navData";
// component imports
import Link from "./Link.astro";
---

<nav aria-label="Primary">
  <div class="navbar">
    <a href="/" class="logomark" aria-label="Go to home page">
      <Icon name="logomark" width="60" />
    </a>
    <ul>
      {navData.map((item) => <Link text={item.name} href={item.path} style="primary" isFilled={false} />)}
    </ul>
  </div>
</nav>
